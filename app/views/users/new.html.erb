<%= form_with model: @user, url: users_path, method: :post do |form| %>
  お名前: <%= form.text_field :name, required: true %><br>
  メールアドレス: <%= form.email_field :email, required: true %><br>
  パスワード: <%= form.password_field :password, required: true %><br>
  パスワード（確認）: <%= form.password_field :password_confirmation, required: true %><br>
  <%= form.submit '登録' %>
<% end %>
